[alias]
	ac = commit -S -am
	s = status
	cm = commit -m
	c = clone
	lg = log --oneline --graph
	fomo = "!f() { git fetch origin ${1:-main} && git rebase origin/${1:-main}; }; f"
	yolo = push -f
	fl = push --force-with-lease

